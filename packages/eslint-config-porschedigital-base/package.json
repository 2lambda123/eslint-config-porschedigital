{
  "name": "@porscheofficial/eslint-config-porschedigital-base",
  "private": true,
  "version": "1.0.0",
  "description": "This package provides Porsche Digital's eslint-config as an extensible shared config.",
  "main": "index.js",
  "scripts": {
    "eslint-find-option-rules": "eslint-find-rules index.js",
    "eslint:ci": "eslint ./ --ext .js,.jsx,.tsx,.ts -f stylish",
    "eslint:fix": "eslint ./ --fix --ext .js,.jsx,.tsx,.ts -f stylish",
    "lint": "eslint --report-unused-disable-directives ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/porscheofficial/eslint-config-porschedigital",
    "directory": "packages/prettier-config-porschedigital"
  },
  "files": [
    "package.json",
    "tsconfig.json",
    "index.js",
    "rules/"
  ],
  "devDependencies": {
    "eslint": "8.7.0",
    "eslint-find-rules": "4.1.0",
    "typescript": "4.5.4"
  },
  "peerDependencies": {
    "eslint": "8",
    "typescript": "*"
  },
  "engines": {
    "node": ">= 12"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "confusing-browser-globals": "1.0.11",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4"
  }
}
